{
  "name": "kdt-pwa-project-shared",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "format:check": "prettier --check \"**/*.{ts,json}\"",
    "type-check": "tsc --noEmit",
    "check-all": "yarn lint && yarn format:check && yarn type-check"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=20.9.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {}
}